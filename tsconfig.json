{
    "compilerOptions": {
        "target": "ES2020",
        "useDefineForClassFields": true,
        "lib": ["ES2020", "DOM", "DOM.Iterable"],
        "module": "ESNext",
        "skipLibCheck": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "jsx": "react",
    },
}
